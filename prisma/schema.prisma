generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dish {
  id          String   @id @default(cuid())
  name        String
  description String
  image       String
  price       Float
  category    String
  createdAt   DateTime @default(now())
}
